{
  "name": "moving-together",
  "version": "v1.0.0-beta-20210219",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && npm run timestamp && ng build --prod && cp _redirects dist/",
    "build-prod2": "npm run env -s && npm run timestamp && ng build --prod -c=prod2 && cp _redirects dist/",
    "build-test": "npm run env -s && npm run timestamp && ng build -c=test && cp _redirects dist/",
    "build-dev": "npm run env -s && npm run timestamp && ng build -c=dev && cp _redirects dist/",
    "start": "npm run env -s && ng serve --proxy-config proxy.conf.js",
    "start-dev": "npm run env -s && npm run timestamp && ng serve --proxy-config proxy.conf.js",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "generate": "ng generate",
    "timestamp": "node ./src/environments/timestamp.js",
    "deploy:dev": "netlify build && netlify deploy",
    "deploy:prod": "netlify build && netlify deploy --prod"
  },
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/platform-server": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@auth0/auth0-spa-js": "^1.16.1",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@ng-bootstrap/ng-bootstrap": "^5.3.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "@ngx-translate/core": "^11.0.1",
    "@popperjs/core": "^2.9.2",
    "@types/core-js": "^2.5.4",
    "@types/jwt-decode": "^2.2.1",
    "@types/socket.io-client": "^1.4.36",
    "agora-rtc-sdk-ng": "^4.7.2",
    "async-mutex": "^0.2.6",
    "bootstrap": "^4.6.0",
    "caniuse-lite": "^1.0.30001328",
    "core-js": "^3.15.2",
    "detect-browser": "^5.2.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.6.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "moment-range": "^4.0.2",
    "moment-timezone": "^0.5.33",
    "replace-in-file": "^6.2.0",
    "rxjs": "^6.6.7",
    "socket.io": "^2.4.1",
    "socket.io-client": "^2.4.0",
    "socket.io-json-parser": "^2.1.1",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.0.4",
    "@angular-devkit/build-angular": "^0.801.0",
    "@angular/cli": "~8.1.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-rocket/scripts": "^4.0.0",
    "@types/jasmine": "^3.8.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/jest": "^24.0.12",
    "@types/jquery": "^3.5.6",
    "@types/lodash": "^4.14.171",
    "@types/luxon": "^1.27.1",
    "@types/node": "^10.17.60",
    "codelyzer": "^5.1.0",
    "hads": "^1.7.0",
    "htmlhint": "^0.11.0",
    "https-proxy-agent": "^2.0.0",
    "husky": "^3.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.7.1",
    "jest-preset-angular": "^7.1.0",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0",
    "protractor": "~5.4.0",
    "puppeteer": "^1.17.0",
    "stylelint": "~10.1.0",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-recommended-scss": "~3.3.0",
    "stylelint-config-standard": "~18.3.0",
    "stylelint-scss": "~3.9.0",
    "ts-node": "^8.3.0",
    "tslint": "~5.18.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "<3.6.0"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  }
}
